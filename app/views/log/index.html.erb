<div class="row">
  <div class="col-sm-12">
    <section class="panel">
      <header class="panel-heading">
        General Table
        <span class="tools pull-right">
            <a href="javascript:;" class="fa fa-chevron-down"></a>
            <a href="javascript:;" class="fa fa-cog"></a>
            <a href="javascript:;" class="fa fa-times"></a>
        </span>
      </header>
      <div class="panel-body">
        <table class="table  table-hover general-table">
          <thead>
          <tr>
            <th>時間</th>
            <th>客戶端 IP</th>
            <th>機器序號</th>
            <th>事件</th>
          </tr>
          </thead>
          <tbody>
          <% @logs.each do |log| %>
          <tr>
            <td><%= log.DateTime %></td>
            <td><%= log.RemoteIP %></td>
            <td><%= log.DeviceSN %></td>
            <td><%= trans_status log.CmdType %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
      <%= will_paginate @logs %>
    </div>
  </section>
</div>
